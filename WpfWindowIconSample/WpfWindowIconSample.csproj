<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net8.0-windows</TargetFramework>
    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
    <UseWPF>true</UseWPF>
    <ApplicationManifest>app.manifest</ApplicationManifest>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="Icon\icon-0.png" />
    <None Remove="Icon\icon-1.png" />
    <None Remove="Icon\icon-2.png" />
  </ItemGroup>

  <ItemGroup>
    <Resource Include="Icon\icon-2.png" />
  </ItemGroup>

  <ItemGroup>
    <Resource Include="Icon\icon-0.png" />
    <Resource Include="Icon\icon-1.png" />
  </ItemGroup>

</Project>
